<% @distriks.each_with_index do |distrik, i| %>
  <tr>
    <td class="hidden-phone"><%= params[:page] ? (params[:page].to_i*10+i-9) : i+1 %></td>
    <td class="hidden-phone"><%= distrik.distributor_code rescue '' %></td>
    <td class="hidden-phone"><%= distrik.distributor_name rescue '' %></td>
    <td><span class="name"><%= distrik.code rescue ''%></span></td>
    <td class="hidden-phone"><%= distrik.name rescue ''%></td>
    <td class="hidden-phone">
      <%= link_to raw('<i class="fa fa-pencil"></i>'), edit_distrik_path(distrik), class: "btn btn-primary btn-xs" %>
      <%= link_to raw('<i class="fa fa-trash-o"></i>'), 'javascript:void(0)', onclick: "delete_one(#{distrik.id}, 'distriks')", class: "btn btn-danger btn-xs" %>
    </td>
  </tr>
<% end %>
<tr>
  <td colspan=8>
    <%= will_paginate @distriks %>
  </td>
</tr>