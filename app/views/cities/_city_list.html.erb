<% @cities.each_with_index do |city, i| %>
  <tr>
    <td class="hidden-phone"><%= params[:page] ? (params[:page].to_i*10+i-9) : i+1 %></td>
    <td class="hidden-phone"><%= city.wilayah_code rescue '' %></td>
    <td class="hidden-phone"><%= city.wilayah_name rescue '' %></td>
    <td class="hidden-phone"><%= city.province_code rescue '' %></td>
    <td class="hidden-phone"><%= city.province_name rescue '' %></td>
    <td><span class="name"><%= city.code rescue ''%></span></td>
    <td class="hidden-phone"><%= city.name rescue ''%></td>
    <td class="hidden-phone nowrap">
      <%= link_to raw('<i class="fa fa-pencil"></i>'), edit_city_path(city), class: "btn btn-primary btn-xs" %>
      <%= link_to raw('<i class="fa fa-trash-o"></i>'), 'javascript:void(0)', onclick: "delete_one(#{city.id}, 'cities')", class: "btn btn-danger btn-xs" %>
    </td>
  </tr>
<% end %>
<tr>
  <td colspan=8>
    <%= will_paginate @cities %>
  </td>
</tr>