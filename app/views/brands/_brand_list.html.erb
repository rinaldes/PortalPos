<% @brands.each_with_index do |brand, i| %>
  <tr>
    <td class="hidden-phone"><%= params[:page] ? (params[:page].to_i*10+i-9) : i+1 %></td>
    <td class="hidden-phone"><%= brand.division_code rescue '' %></td>
    <td class="hidden-phone"><%= brand.division_name rescue '' %></td>
    <td class="hidden-phone"><%= brand.category_code rescue '' %></td>
    <td class="hidden-phone"><%= brand.category_name rescue '' %></td>
    <td><span class="name"><%= brand.code rescue ''%></span></td>
    <td class="hidden-phone"><%= brand.name rescue ''%></td>
    <td class="hidden-phone">
      <%= link_to edit_brand_path(brand), class: "btn btn-primary btn-xs" do %>
        <i class="fa fa-pencil"> Edit</i>
      <% end %>
      <%= link_to 'javascript:void(0)', onclick: "delete_one(#{brand.id}, 'brands')", class: "btn btn-danger btn-xs" do %>
        <i class="fa fa-trash-o"> Delete</i>
      <% end %>
    </td>
  </tr>
<% end %>
<tr>
  <td colspan=8>
    <%= will_paginate @brands %>
  </td>
</tr>