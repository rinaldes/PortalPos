<% @divisions.each_with_index do |division, i| %>
  <tr>
    <td class="hidden-phone"><%= params[:page] ? (params[:page].to_i*10+i-9) : i+1 %></td>
    <td><span class="name"><%= division.code rescue ''%></span></td>
    <td class="hidden-phone"><%= division.name rescue ''%></td>
    <td class="hidden-phone"><%= division.principal_code rescue '' %></td>
    <td class="hidden-phone"><%= division.principal_name rescue '' %></td>
    <td class="hidden-phone">
      <%= link_to raw('<i class="fa fa-pencil"></i>'), edit_division_path(division), class: "btn btn-primary btn-xs" %>
      <%= link_to raw('<i class="fa fa-trash-o"></i>'), 'javascript:void(0)', onclick: "delete_one(#{division.id}, 'divisions')", class: "btn btn-danger btn-xs" %>
    </td>
  </tr>
<% end %>
<tr>
  <td colspan=8>
    <%= will_paginate @divisions %>
  </td>
</tr>