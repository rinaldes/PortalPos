<% @type_ulinons.each_with_index do |type_ulinon, i| %>
  <tr>
    <td class="hidden-phone"><%= params[:page] ? (params[:page].to_i*10+i-9) : i+1 %></td>
    <td><span class="name"><%= type_ulinon.code rescue ''%></span></td>
    <td class="hidden-phone"><%= type_ulinon.name rescue ''%></td>
    <td class="hidden-phone">
      <%= link_to edit_type_ulinon_path(type_ulinon), class: "btn btn-primary btn-xs" do %>
        <i class="fa fa-pencil"> Edit</i>
      <% end %>
      <%= link_to 'javascript:void(0)', onclick: "delete_one(#{type_ulinon.id}, 'type_ulinons')", class: "btn btn-danger btn-xs" do %>
        <i class="fa fa-trash-o"> Delete</i>
      <% end %>
    </td>
  </tr>
<% end %>
<tr>
  <td colspan=8>
    <%= will_paginate @type_ulinons %>
  </td>
</tr>