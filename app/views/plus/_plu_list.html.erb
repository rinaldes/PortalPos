<% @plus.each_with_index do |plu, i| %>
  <tr>
    <td class="hidden-phone"><%= params[:page] ? (params[:page].to_i*10+i-9) : i+1 %></td>
    <td><span class="name"><%= plu.code rescue ''%></span></td>
    <td class="hidden-phone"><%= plu.sku_code rescue ''%></td>
    <td class="hidden-phone"><%= plu.sku_name rescue ''%></td>
    <td class="hidden-phone"><%= plu.division_name rescue '' %></td>
    <td class="hidden-phone"><%= plu.category_name rescue '' %></td>
    <td class="hidden-phone"><%= plu.brand_name rescue '' %></td>
    <td class="hidden-phone"><%= plu.h_jual rescue '' %></td>
    <td class="hidden-phone"><%= plu.h_beli rescue '' %></td>
    <td class="hidden-phone"><%= plu.satuan rescue '' %></td>
    <td class="hidden-phone">
      <%= link_to edit_plu_path(plu), class: "btn btn-primary btn-xs" do %>
        <i class="fa fa-pencil"> Edit</i>
      <% end %>
      <%= link_to 'javascript:void(0)', onclick: "delete_one(#{plu.id}, 'plus')", class: "btn btn-danger btn-xs" do %>
        <i class="fa fa-trash-o"> Delete</i>
      <% end %>
    </td>
  </tr>
<% end %>
<tr>
  <td colspan=8>
    <%= will_paginate @plus %>
  </td>
</tr>