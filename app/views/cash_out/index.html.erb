<div id="ribbon">
  <ol class="breadcrumb">
    <li>Data Master</li><li>Dana Keluar</li><li>List</li>
  </ol>
</div>
<div id="content">
  <section id="widget-grid" class="">
    <div class="row">
      <article class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
        <div class="jarviswidget jarviswidget-color-blueDark" id="wid-id-1" data-widget-editbutton="false"><!-- Widget ID (each widget will need unique ID)-->
        <%= render template: 'tabs/cash' %>
          <div>
            <div class="widget-body no-padding">
              <table class="table table-striped table-bordered" width="100%">
                <thead>
                  <tr>
                    <th>No</th>
                    <th>
                      Jumlah Dana
                      <span style="float: right;">
                        <%= link_to 'javascript:void(0)', id: 'cash_value-desc', class: 'sorting hide' do %><i class="fa fa-sort-desc"></i><% end %>
                        <%= link_to 'javascript:void(0)', id: 'cash_value-asc', class: 'sorting' do %><i class="fa fa-sort-asc"></i><% end %>
                      </span>
                    </th>
                    <th>
                      Deskripsi
                      <span style="float: right;">
                        <%= link_to 'javascript:void(0)', id: 'description-desc', class: 'sorting hide' do %><i class="fa fa-sort-desc"></i><% end %>
                        <%= link_to 'javascript:void(0)', id: 'description-asc', class: 'sorting' do %><i class="fa fa-sort-asc"></i><% end %>
                      </span>
                    </th>
                    <th>
                      Jurnal ID
                      <span style="float: right;">
                        <%= link_to 'javascript:void(0)', id: 'jurnal_id-desc', class: 'sorting hide' do %><i class="fa fa-sort-desc"></i><% end %>
                        <%= link_to 'javascript:void(0)', id: 'jurnal_id-asc', class: 'sorting' do %><i class="fa fa-sort-asc"></i><% end %>
                      </span>
                    </th>
                    <th>
                      Number ID
                      <span style="float: right;">
                        <%= link_to 'javascript:void(0)', id: 'number_id-desc', class: 'sorting hide' do %><i class="fa fa-sort-desc"></i><% end %>
                        <%= link_to 'javascript:void(0)', id: 'number_id-asc', class: 'sorting' do %><i class="fa fa-sort-asc"></i><% end %>
                      </span>
                    </th>

                    <th>
                      Tanggal Transaksi
                      <span style="float: right;">
                        <%= link_to 'javascript:void(0)', id: 'transaction_date-desc', class: 'sorting hide' do %><i class="fa fa-sort-desc"></i><% end %>
                        <%= link_to 'javascript:void(0)', id: 'transaction_date-asc', class: 'sorting'  do %><i class="fa fa-sort-asc"></i><% end %>
                      </span>
                    </th>

                    <th>
                      Transaksi
                      <span style="float: right;">
                        <%= link_to 'javascript:void(0)', id: 'transaction_code-desc', class: 'sorting hide' do %><i class="fa fa-sort-desc"></i><% end %>
                        <%= link_to 'javascript:void(0)', id: 'transaction_code-asc', class: 'sorting' do %><i class="fa fa-sort-asc"></i><% end %>
                      </span>
                    </th>

                    <th>
                      Pengguna
                      <span style="float: right;">
                        <%= link_to 'javascript:void(0)', id: 'username-desc', class: 'sorting hide' do %><i class="fa fa-sort-desc"></i><% end %>
                        <%= link_to 'javascript:void(0)', id: 'username-asc', class: 'sorting' do %><i class="fa fa-sort-asc"></i><% end %>
                      </span>
                    </th>

                  </tr>

                  <tr>
                    <th></th>
                    <th class="hasinput">
                      <%= text_field_tag :cash_value, params[:cash_value], class: 'form-control search', name: 'cash_outs.cash_value' %>
                    </th>
                    <th class="hasinput">
                      <%= text_field_tag :description, params[:description], class: 'form-control search', name: 'cash_outs.description' %>
                    </th>
                    <th class="hasinput">
                      <%= text_field_tag :jurnal_id, params[:jurnal_id], class: 'form-control search', name: 'cash_outs.jurnal_id' %>
                    </th>
                    <th class="hasinput">
                      <%= text_field_tag :number_id, params[:number_id], class: 'form-control search', name: 'cash_outs.number_id' %>
                    </th>
                    <th class="hasinput">
                      <%= text_field_tag :transaction_date, params[:transaction_date], class: 'form-control search', name: 'cash_outs.transaction_date' %>
                    </th>
                    <th class="hasinput">
                      <%= text_field_tag :transaction_code, params[:transaction_code], class: 'form-control search', name: 'sale_transaction.transaction_number' %>
                    </th>
                    <th class="hasinput">
                      <%= text_field_tag :username, params[:username], class: 'form-control search', name: 'users.username' %>
                    </th>
                    
                  </tr>
                </thead>
                <tbody id='cash_out-list'><%= render partial: 'cash_in/cash_in_list', locals: {cash: @cash_outs} %></tbody>
              </table>
            </div>
          </div>
        </div>
      </article>
    </div>
  </section>
</div>